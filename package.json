  "scripts": {
    "dev": "npm run prepare:demo -- dev && npm run build:dev && webpack serve --open --mode development",
    "watch": "webpack --watch --mode development",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "test": "echo \"no test specified\" && exit 0",
    "types:check": "tsc src/index.ts src/services/types/global.d.ts --jsx react --jsxFactory h --noEmit --target ESNext --moduleResolution node",
