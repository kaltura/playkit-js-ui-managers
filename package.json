    "lint:fix": "npm run lint:check -- --fix",
    "prettier:fix": "prettier --write .",
    "release": "standard-version",
    "pushTaggedRelease": "git push --follow-tags --no-verify origin master",
    "prepare:demo": "sh scripts/prepare-demo.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
